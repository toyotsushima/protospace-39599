<main class="main">
  <div class="inner">
   <%# ログインしているときは以下を表示する %>
      <% if user_signed_in? %>
     <div class="greeting">
     こんにちわ、
       <%= link_to "#{current_user.name}", "/users/#{current_user.id}", class: :greeting__link %>
        さん
      </div> 
      <% else %> 
      <div class="grid-6">
            <%= link_to "", new_user_session_path, class: "post" %>
            <%= link_to "", new_user_registration_path, class: "post" %>
          </div>
        <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
